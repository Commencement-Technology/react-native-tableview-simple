{
  "name": "react-native-tableview-simple",
  "description": "React Native component for TableView made with pure CSS",
  "homepage": "https://github.com/Purii/react-native-tableview-simple",
  "version": "0.16.4",
  "author": "Patrick Puritscher <hello@patrickpuritscher.com>",
  "scripts": {
    "format": "prettier --trailing-comma es5 --single-quote --write 'components/**/*.js'",
    "jest": "jest",
    "lint": "eslint index.js components/",
    "precommit": "lint-staged",
    "test": "npm run lint && npm run jest"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/example/"
    ]
  },
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "react-native",
    "react-component",
    "tableview",
    "UITableView",
    "crossplatform",
    "ios",
    "android"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": ">=0.35 || 0.43.0-rc"
  },
  "bugs": {
    "url": "https://github.com/Purii/react-native-tableview-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Purii/react-native-tableview-simple.git"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.15.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-plugin-react": "^6.10.0",
    "husky": "^0.13.2",
    "jest": "18.1.0",
    "lint-staged": "^3.3.1",
    "prettier": "^0.22.0",
    "react": "^15.4.2",
    "react-native": "^0.41.2",
    "react-test-renderer": "15.4.2"
  }
}
